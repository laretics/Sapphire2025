{
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database={my_database};User={my_user};Password={myPassword};",
    "RemoteConnection": "server={my_server}};port=3306;database={myDatabase};user={my_user};password={my_password};"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
